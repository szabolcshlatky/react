{
  "name": "create-react-app",
  "description": "A legacy fullstack project's frontend part",
  "keywords": [
    "React",
    "TypeScript",
    "Redux",
    "Class Components",
    "React Router",
    "Firebase 9"
  ],
  "author": "Coding Sable",
  "license": "ISC",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "create-react-app"
  },
  "main": "index.tsx",
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "json": "json-server --watch src/data/db.json --port 3001"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.91",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "link": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "9.1.0",
    "react-router-dom": "6.22.3",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@jest/types": "29.6.3",
    "@types/autoprefixer-core": "^5.1.34",
    "@types/eslint": "^8.56.6",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/jsdom": "^21.1.6",
    "@types/json-server": "^0.14.7",
    "@types/lodash": "^4.17.0",
    "@types/sass-loader": "^8.0.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cssnano": "^6.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-love": "^43.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "firebase": "9.23.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jsdom": "^24.0.0",
    "json-server": "^1.0.0-alpha.23",
    "lodash": "^4.17.21",
    "postcss": "^8.4.38",
    "postcss-preset-env": "9.3.0",
    "postcss-scss": "4.0.9",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "sass-loader": "13.3.3",
    "uuid": "^9.0.1"
  }
}
