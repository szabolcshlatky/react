{
  "name": "react-guide",
  "description": "Checkout the branches for different topics",
  "keywords": [
    "React-Router-DOM"
  ],
  "author": "Coding Sable",
  "license": "ISC",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "react-guide"
  },
  "main": "main.tsx",
  "browserslist": {
    "production": [
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true
  },
  "engines": {
    "pnpm": "^8.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint && test"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "echo": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "pnpm run test:jest && pnpm run test:vitest",
    "prepare": "husky install",
    "json": "json-server --watch src/data/db.json --port 3001"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0-rc.0",
    "@jest/types": "29.6.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/autoprefixer-core": "^5.1.34",
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^27.5.2",
    "@types/jsdom": "^21.1.6",
    "@types/json-server": "^0.14.7",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cssnano": "^6.1.2",
    "eslint": "^9.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "15.0.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jsdom": "^24.0.0",
    "json-server": "^1.0.0-alpha.23",
    "localforage": "1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "6.3.4",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "7.4.0",
    "uuid": "^9.0.1",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  }
}
